{
   "background": {
      "page": "background.html",
      "persistent": false
   },
   "browser_action": {
      "default_icon": "static/icon19.png",
      "default_popup": "browser_action.html",
      "default_title": "דרדס"
   },
   "content_scripts": [ {
      "all_frames": true,
      "js": [ "content.js" ],
      "matches": [ "<all_urls>" ]
   } ],

   "description": "Der-Die-Das-ize German nouns",
   "icons": {
      "128": "static/icon128.png",
      "16": "static/icon16.png",
      "32": "static/icon32.png",
      "48": "static/icon48.png"
   },
   "manifest_version": 2,
   "name": "דרדס",
   "permissions": ["tabs"],

   "version": "0.0.1",
   "web_accessible_resources": [ "content.css" ]
}
